<header>
  <div class="title">
    <h1><%= @error.get('title') %></h1>
    <span class="badge badge-<%= @error.css_warning_level() %>">
      <%= @error.get('no') %>
    </span>
  </div>
  <div class="summary">
    <em>#<%= @error.get('crc') %></em><br>
    <time class="timeago" datetime="<%= @error.get('error_info')[0].occured_at %>"><%= @error.get('error_info')[0].occured_at %></time>
  </div>
</header>
